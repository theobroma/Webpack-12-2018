{
    "name": "React-es6-starter",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "start": "npm run dev",
        "dev": "webpack-dev-server --history-api-fallback --inline --progress --colors --port 3000 --config webpack.dev.js",
        "start:devwrite": "webpack-dev-server --history-api-fallback --inline --progress --colors --port 3000 --config webpack.dev.write.js",
        "build:stats": "webpack --env production --profile --progress --json > stats.json",
        "build": "npm run clean && npm run build:prod",
        "build:prod": "webpack --config webpack.prod.js",
        "dll": "webpack --config webpack.dll.config.babel.js",
        "clean": "rimraf ./dist",
        "eslint": "./node_modules/.bin/eslint **/*.js",
        "eslint:fix": "./node_modules/.bin/eslint --fix **/*.js",
        "test": "NODE_ENV=test ENV=test jest"
    },
    "jest": {
        "setupTestFrameworkScriptFile": "./setupTests.js",
        "moduleFileExtensions": [
            "js",
            "jsx"
        ],
        "moduleDirectories": [
            "src/app",
            "node_modules"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "testResultsProcessor": "./node_modules/jest-junit-reporter",
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "webpack.config.js"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 0,
                "functions": 0,
                "lines": 0,
                "statements": 0
            }
        }
    },
    "dependencies": {
        "@storybook/react": "^4.1.3",
        "axios": "^0.18.0",
        "classnames": "^2.2.6",
        "enzyme-to-json": "^3.3.5",
        "highlight.js": "^9.13.1",
        "history": "^4.7.2",
        "jest": "^23.6.0",
        "jest-junit-reporter": "^1.1.0",
        "lodash": "^4.17.11",
        "marked": "^1.1.1",
        "next": "^7.0.2",
        "polished": "^2.3.1",
        "prettier": "^1.15.3",
        "pretty-ms": "^4.0.0",
        "prismjs": "^1.15.0",
        "prop-types": "^15.6.2",
        "react": "^16.7.0",
        "react-countdown-now": "^1.3.0",
        "react-dom": "^16.7.0",
        "react-highlight": "^0.12.0",
        "react-highlight.js": "^1.0.7",
        "react-markdown": "^4.0.4",
        "react-prism": "^4.3.2",
        "react-redux": "^6.0.0",
        "react-render-html": "^0.6.0",
        "react-router-dom": "^4.3.1",
        "react-vis": "^1.11.5",
        "recharts": "^1.4.1",
        "recompose": "^0.30.0",
        "redux": "^4.0.1",
        "redux-devtools-extension": "^2.13.7",
        "redux-logger": "^3.0.6",
        "redux-promise": "^0.6.0",
        "redux-thunk": "^2.3.0",
        "semantic-ui-react": "^0.84.0",
        "styled-components": "^4.1.3",
        "uuid": "^3.3.2",
        "validatorjs": "^3.15.1"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.2.3",
        "@babel/preset-env": "^7.2.3",
        "@babel/preset-react": "^7.0.0",
        "add-asset-html-webpack-plugin": "^3.1.2",
        "autoprefixer": "^9.4.3",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.4",
        "clean-webpack-plugin": "^1.0.0",
        "copy-webpack-plugin": "^4.6.0",
        "css-loader": "^2.0.1",
        "css-mqpacker": "^7.0.0",
        "cssnano": "^4.1.7",
        "dotenv-webpack": "^1.5.7",
        "enzyme": "^3.8.0",
        "enzyme-adapter-react-16": "^1.7.1",
        "eslint": "^5.10.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^3.3.0",
        "eslint-import-resolver-webpack": "^0.10.1",
        "eslint-loader": "^2.1.1",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jest": "^22.1.2",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-prettier": "^3.0.0",
        "eslint-plugin-react": "^7.11.1",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "image-webpack-loader": "^4.6.0",
        "node-sass": "^4.11.0",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "postcss-assets": "^5.0.0",
        "postcss-cssnext": "^3.1.0",
        "postcss-custom-media": "^7.0.7",
        "postcss-flexbugs-fixes": "^4.1.0",
        "postcss-fontpath": "^1.0.0",
        "postcss-loader": "^3.0.0",
        "postcss-media-minmax": "^4.0.0",
        "postcss-reporter": "^6.0.0",
        "prettier-eslint": "^8.8.2",
        "raf": "^3.4.1",
        "react-hot-loader": "^4.6.3",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "uglifyjs-webpack-plugin": "^2.0.1",
        "webpack": "^4.28.1",
        "webpack-bundle-analyzer": "^3.0.3",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.10",
        "webpack-merge": "^4.1.5",
        "write-file-webpack-plugin": "^4.5.0"
    }
}
